{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "sumo",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#fields"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.entity.function.sumo",
				"match": "\\b(geo lookup|haversine|hexToDec|if|in|ipv4ToNumber|isBlank|isEmpty|isNull|isNumeric|isPrivateIP|isPublicIP|isValidIP|join|length|limit|logcompare|logreduce|lookup|luhn (credit card validator)|matches|median|merge|now|num|outlier|parseHex|predict|replace|rollingstd|save|sessionize|smooth|sort|substring|timeslice|toLowerCase and toUpperCase|top|total|trace|transaction|transactionize|transpose|urldecode|urlencode|wher)\\b"
			}]
		},
		"fields": {
			"patterns": [{
				"name": "keyword.variable.other.sumo",
				"match": "\\b(_collector|_messageCount|_messageTime|_raw|_receiptTime|_size|_source|_sourceCategory|_sourceHost|_sourceName|_format)\\b"
			}]
		},
		"comments": {
			"patterns": [{
				"name": "comment.line.double-slash",
				"match": "\/\/.*"
			}]
		},
		"strings": {
			"name": "string.quoted.double.sumo",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.sumo",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.sumo"
}
